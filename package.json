{
  "name": "vue3-api-demo",
  "version": "0.2.5",
  "author": {
    "name": "zmtlwzy"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev:nw": "vite --host",
    "build": "vite build",
    "preview": "vite build --mode preview && vite preview",
    "preview:dist": "vite preview --host",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "reinstall": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm bootstrap"
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "highlight.js": "^11.1.0",
    "lodash-es": "latest",
    "pinia": "2.0.0-beta.5",
    "vue": "next",
    "vue-router": "next"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitejs/plugin-vue-jsx": "latest",
    "@vue/compiler-sfc": "latest",
    "less": "latest",
    "naive-ui": "latest",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "vfonts": "latest",
    "vite": "^2.4.4",
    "vite-plugin-components": "latest",
    "vite-plugin-windicss": "latest",
    "windicss": "^3.1.6"
  },
  "license": "MIT"
}
