{
  "name": "vue3-api-demo",
  "version": "0.2.7",
  "author": {
    "name": "zmtlwzy"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite build && vite preview --host",
    "preview:dist": "vite preview --host",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lock": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json",
    "clean:lib": "rimraf node_modules",
    "update:lib": "yarn clean:lock && yarn",
    "reinstall": "yarn clean:lock && yarn clean:lib && yarn"
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "highlight.js": "latest",
    "lodash-es": "latest",
    "pinia": "next",
    "vue": "next",
    "vue-router": "next"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.397",
    "@types/node": "latest",
    "@vitejs/plugin-vue": "latest",
    "@vitejs/plugin-vue-jsx": "latest",
    "@vue/compiler-sfc": "latest",
    "less": "latest",
    "naive-ui": "latest",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "latest",
    "unplugin-icons": "latest",
    "unplugin-vue-components": "latest",
    "vfonts": "latest",
    "vite": "latest",
    "vite-plugin-windicss": "latest",
    "windicss": "latest"
  },
  "license": "MIT"
}
