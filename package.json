{
  "name": "vue3-api-demo",
  "version": "0.3.0",
  "author": {
    "name": "zmtlwzy"
  },
  "scripts": {
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite build && vite preview --host",
    "preview:dist": "vite preview --host",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lock": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json",
    "clean:lib": "rimraf node_modules",
    "clean": "npm run clean:lock && npm run clean:lib",
    "prepare": "husky install",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@formily/core": "latest",
    "@formily/vue": "latest",
    "@vueuse/core": "latest",
    "highlight.js": "^11.3.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.6",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@iconify/json": "^1.1.444",
    "@types/node": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@unocss/preset-icons": "latest",
    "@unocss/reset": "latest",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vitejs/plugin-vue-jsx": "^1.3.2",
    "@vue/compiler-sfc": "^3.2.26",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "less": "^4.1.2",
    "lint-staged": "^12.1.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "naive-ui": "^2.23.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "unocss": "latest",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-icons": "^0.12.23",
    "unplugin-vue-components": "^0.17.9",
    "vfonts": "^0.1.0",
    "vite": "^2.7.3"
  },
  "license": "MIT"
}
