{
  "name": "vue3-api-demo",
  "version": "0.2.5",
  "author": {
    "name": "zmtlwzy",
    "url": "https://github.com/anncwb"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite",
    "dev:nw": "vite --host",
    "build": "vite build",
    "preview": "vite build --mode preview && vite preview",
    "preview:dist": "vite preview --host",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "reinstall": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm bootstrap"
  },
  "dependencies": {
    "@vueuse/core": "^5.1.3",
    "highlight.js": "^11.1.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.0-beta.5",
    "vue": "^3.1.4",
    "vue-router": "^4.0.10"
  },
  "devDependencies": {
    "@types/node": "^16.3.2",
    "@vitejs/plugin-vue": "^1.2.5",
    "@vitejs/plugin-vue-jsx": "^1.1.6",
    "@vue/compiler-sfc": "^3.1.4",
    "less": "^4.1.1",
    "naive-ui": "^2.15.4",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "vfonts": "^0.1.0",
    "vite": "^2.4.2",
    "vite-plugin-components": "^0.12.2",
    "vite-plugin-windicss": "^1.2.4"
  },
  "license": "MIT"
