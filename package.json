{
  "name": "vue3-api-demo",
  "version": "0.3.5",
  "author": {
    "name": "zmtlwzy"
  },
  "scripts": {
    "release": "standard-version",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "dev": "vite --host",
    "build": "vite build",
    "build:vercel": "vite build -m vercel",
    "preview": "vite build && vite preview --host",
    "preview:vercel": "vite preview --host --base \"/\"",
    "preview:dist": "vite preview --host",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"./**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lock": "rimraf yarn.lock && rimraf pnpm-lock.yaml && rimraf package.lock.json",
    "clean:lib": "rimraf node_modules",
    "clean": "npm run clean:lock && npm run clean:lib",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "highlight.js": "^11.4.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.11",
    "vue": "^3.2.29",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@iconify/json": "^2.0.33",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@unocss/preset-icons": "latest",
    "@unocss/reset": "latest",
    "@vitejs/plugin-vue": "^2.1.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "@vue/compiler-sfc": "^3.2.29",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.2.3",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "less": "^4.1.2",
    "lint-staged": "^12.3.3",
    "naive-ui": "^2.24.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5",
    "unocss": "latest",
    "unplugin-auto-import": "^0.5.11",
    "unplugin-icons": "^0.13.0",
    "unplugin-vue-components": "^0.17.15",
    "vfonts": "^0.1.0",
    "vite": "^2.7.13"
  },
  "license": "MIT"
}
